{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug OllamaMessenger",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/OllamaMessenger",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build OllamaMessenger",
      "linux": {
        "miDebuggerPath": "/usr/bin/gdb"
      },
      "osx": {
        "miDebuggerPath": "/usr/bin/lldb"
      },
      "windows": {
        "miDebuggerPath": "C:/MinGW/bin/gdb.exe"
      }
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/beta/Documents/GitHub/OllamaMessenger",
      "program": "/home/beta/Documents/GitHub/OllamaMessenger/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}